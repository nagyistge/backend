{
    "name": "timegridio/backend",
    "type": "library",
    "description": "Backend package for timegrid booking app",
    "keywords": [
        "alariva",
        "Timegrid Backend"
    ],
    "homepage": "https://github.com/timegridio/backend",
    "license": "AGPL",
    "authors": [
        {
            "name": "Ariel Vallese",
            "email": "alariva@timegrid.io",
            "homepage": "http://timegrid.io",
            "role": "Founder & Developer"
        }
    ],
    "require": {
        "alariva/timegrid": "~4.0",
        "illuminate/support": "~5.2",
        "php" : ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Timegridio\\Backend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Timegridio\\Backend\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
